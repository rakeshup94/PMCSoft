<!DOCTYPE html>
<html lang="en" data-footer="true">
<head>
    <title>IEXPRO ERP</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="">
  
    <!--Loading bootstrap css-->
    <link type="text/css" rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,700">
    <link type="text/css" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Oswald:400,700,300">

    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />



    @if (IsSectionDefined("styles"))
    {
        @RenderSection("styles", required: false)
    }


    <!--Loading style-->
    <link type="text/css" rel="stylesheet" href="~/Content/css/style.css">
    <link type="text/css" rel="stylesheet" href="~/Content/css/style-responsive.css">

</head>
<body>
    <form class="was-validated" novalidate="novalidate">

        <div id="header-topbar-option-demo" class="page-header-topbar">
            <nav id="topbar" role="navigation" style="margin-bottom: 0; z-index: 2;"
                 class="navbar navbar-default navbar-static-top">
                <div class="navbar-header">
                    <button type="button" data-bs-toggle="collapse" href="#sidebar-collapse" role="button"
                            aria-expanded="false" aria-controls="sidebar-collapse" class="navbar-toggle">
                        <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar">
                        </span>
                    </button>
                    <a id="logo" href="#" class="navbar-brand">
                        <span class="fa fa-rocket"></span><span class="logo-text">
                            <img src="~/images/logo.png" alt="" />
                        </span>
                        <span style="display: none" class="logo-text-icon">
                            <img src="~/images/logo.png" alt="" />
                        </span>
                    </a>
                </div>
                <div class="topbar-main">
                    <a id="menu-toggle" href="#" class="d-none d-sm-block">
                        <i class="fa fa-bars"></i>
                    </a>
                    <ul class="nav navbar navbar-top-links navbar-right mbn">
                        <li>
                            <img style="max-height: 70px;" src="~/images/logo/247_company.png"
                                 class="img-fluid top-righ-logo" alt="" />
                        </li>
                        <li class="dropdown topbar-user">
                            <a data-hover="dropdown" href="#" class="dropdown-bs-toggle">
                                <img src="~/images/profile.jpg" width="128" height="128" alt=""
                                     class="img-fluid img-circle" />&nbsp;<span class="hidden-xs">

                                </span>&nbsp;<span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-user pull-right">
                                <li><a href="#"><i class="fa fa-user"></i>My Profile</a></li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#" class="dropdown-bs-toggle">
                                        <i class="fa fa-key"></i>Log Out
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        <!--END TOPBAR-->
        <div id="wrapper">
            <!--BEGIN SIDEBAR MENU-->
            <nav id="sidebar" role="navigation" class="navbar-default navbar-static-side">
                <div id="sidebar-collapse" class="menu-scroll collapse d-sm-block">
                    @Html.Partial("_LeftMenu")
                </div>
            </nav>
            <!--END SIDEBAR MENU-->
            <!--BEGIN PAGE WRAPPER-->
            <div id="page-wrapper">
                @RenderBody()

            </div>
            <div id="footer">

                <div class="copyright">
                    <p class="mb-0 text-white text-medium">Copyright Enablesoft ERP Conslustancy pvt ltd</p>
                </div>
            </div>
            <!--END FOOTER-->
        </div>
        <!--END PAGE WRAPPER-->
    </form>
    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/js/bootstrap.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js"></script>
    <script src="~/lib/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/lib/jquery-cookie/jquery.cookie.js"></script>


    @if (IsSectionDefined("scripts"))
    {
        @RenderSection("scripts", required: false)
    }



    <script src="~/Content/Javascripts/main.js"></script>
</body>
</html>
